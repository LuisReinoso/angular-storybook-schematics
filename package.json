{
  "name": "angular-storybook",
  "version": "1.0.0",
  "description": "Angular schematics which automatically adds new components created with `ng generate component` to the [Storybook](https://storybook.js.org/)",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "npm run build && jasmine src/**/*_spec.js",
    "build:watch": "tsc -p tsconfig.json --watch"
  },
  "keywords": [
    "schematics",
    "angular",
    "storybook"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lowxorx/angular-storybook-schematics.git"
  },
  "author": "Lowxorx",
  "license": "MIT",
  "schematics": "./src/collection.json",
  "devDependencies": {
    "@angular-devkit/core": "^8.3.8",
    "@angular-devkit/schematics": "^8.3.8",
    "@schematics/angular": "^8.3.8",
    "@types/babel__generator": "^7.6.0",
    "@types/babel__traverse": "^7.0.7",
    "@types/jasmine": "^3.4.4",
    "@types/node": "^12.7.12",
    "jasmine": "^3.5.0",
    "json5": "^2.1.1",
    "ts-node": "^8.4.1",
    "typescript": "3.6.4"
  },
  "dependencies": {
    "@babel/generator": "^7.6.4",
    "@babel/parser": "^7.6.4",
    "@babel/traverse": "^7.6.3"
  },
  "files": [
    "src/**/files",
    "src/**/*.js",
    "src/**/*.json",
    "src/**/*.d.ts",
    "!src/**/*_spec.js",
    "!src/component/createSchema.js"
  ]
}
